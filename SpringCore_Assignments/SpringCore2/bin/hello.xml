<?xml version="1.0" encoding="UTF-8"?>
<beans xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd" xmlns:p="http://www.springframework.org/schema/p" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.springframework.org/schema/beans">


<bean class="SpringCore2.Question" id="question">

<constructor-arg value="1" index="0"/>

<constructor-arg value="Name any of the two design pattern ?" index="1"/>


<constructor-arg index="2">
          <list>
<value>SingeltonDP</value>

<value>Factory DP</value>

             </list>

</constructor-arg>

<constructor-arg index="3">


<set>

<value>SingeltonDP</value>

<value>Factory DP</value>

</set>

</constructor-arg>


<constructor-arg index="4">


<map>

<entry value="SingeltonDP" key="1"/>

<entry value="Factory DP" key="2"/>

</map>

</constructor-arg>

</bean>

</beans>