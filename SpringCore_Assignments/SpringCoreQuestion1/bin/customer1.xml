<?xml version="1.0" encoding="UTF-8"?>
<beans xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd" xmlns:p="http://www.springframework.org/schema/p" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.springframework.org/schema/beans">

<bean id="cust" class="SpringCoreQ1.Customer">
<property name="customerId" value="22"/>
<property name="customerName" value="vinaya"/>
<property name="customerContact" value="7349568978"/>
<property name="customerAddress" ref="ads"/>
</bean>


<bean id="ads" class="SpringCoreQ1.Address">
<property name="street" value="thodar"/>
<property name="city" value="mangalore"/>
<property name="state" value="karnataka"/>
<property name="zip" value="abcd"/>
<property name="country" value="india"/>
</bean>
</beans>